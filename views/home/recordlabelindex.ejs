<%- include('../includes/head.ejs') %>
<%- include('../includes/navigation.ejs') %>
	<!-- main content -->
	<main class="main">
		<div class="container-fluid">
			<section class="row row--grid">
				<div class="col-12 col-md-6 col-xl-11">
					<div class="row row--grid">
						<!-- title -->
                        <div class="col-12">
                            <div class="main__title season-title">
								<h2><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M21.65,2.24a1,1,0,0,0-.8-.23l-13,2A1,1,0,0,0,7,5V15.35A3.45,3.45,0,0,0,5.5,15,3.5,3.5,0,1,0,9,18.5V10.86L20,9.17v4.18A3.45,3.45,0,0,0,18.5,13,3.5,3.5,0,1,0,22,16.5V3A1,1,0,0,0,21.65,2.24ZM5.5,20A1.5,1.5,0,1,1,7,18.5,1.5,1.5,0,0,1,5.5,20Zm13-2A1.5,1.5,0,1,1,20,16.5,1.5,1.5,0,0,1,18.5,18ZM20,7.14,9,8.83v-3L20,4.17Z"/></svg><a>Top 6 Songs of The Current Season (Dec 1st - Dec 31st)</a></h2>
                                
								<a href="/season/2" class="main__link">See all <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17.92,11.62a1,1,0,0,0-.21-.33l-5-5a1,1,0,0,0-1.42,1.42L14.59,11H7a1,1,0,0,0,0,2h7.59l-3.3,3.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l5-5a1,1,0,0,0,.21-.33A1,1,0,0,0,17.92,11.62Z"/></svg></a>
                            </div>
                        </div>
                        <!-- end title -->

                        <div class="col-12">
                            <% for (let song of currSeasonTopSongs) { %>
								<div id="season-album">
									<div>
										<div class="album__cover">
											<iframe width="560" height="315" src="https://www.youtube.com/embed/<%= song.youtubeSongId %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
										</div>
										<div class="album__title" id="song-season-details">
											<div>
												<h3><a href="/release/<%= song.songTitle %>/<%= song.artistName %>"><%= song.songTitle %></a></h3>
												<span><a href="/artist/<%= song.artistName %>"><%= song.artistName %></a></span>
											</div>
											<div>
												<span class="single-item__time top-votes">Votes: <%= song.votes %></span>
											</div>
										</div>
									</div>
								</div>
                            <% } %>
                        </div>
					</div>
				</div>
			</section>

			<section class="row row--grid">
				<div class="col-12 col-md-6 col-xl-11">
					<div class="row row--grid">
						<!-- title -->
                        <div class="col-12">
                            <div class="main__title season-title">
                                <h2><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M21.65,2.24a1,1,0,0,0-.8-.23l-13,2A1,1,0,0,0,7,5V15.35A3.45,3.45,0,0,0,5.5,15,3.5,3.5,0,1,0,9,18.5V10.86L20,9.17v4.18A3.45,3.45,0,0,0,18.5,13,3.5,3.5,0,1,0,22,16.5V3A1,1,0,0,0,21.65,2.24ZM5.5,20A1.5,1.5,0,1,1,7,18.5,1.5,1.5,0,0,1,5.5,20Zm13-2A1.5,1.5,0,1,1,20,16.5,1.5,1.5,0,0,1,18.5,18ZM20,7.14,9,8.83v-3L20,4.17Z"/></svg><a>Top 6 Songs of The Previous Season (Nov 1st - Nov 30th)</a></h2>

								<a href="/season/1" class="main__link">See all <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17.92,11.62a1,1,0,0,0-.21-.33l-5-5a1,1,0,0,0-1.42,1.42L14.59,11H7a1,1,0,0,0,0,2h7.59l-3.3,3.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l5-5a1,1,0,0,0,.21-.33A1,1,0,0,0,17.92,11.62Z"/></svg></a>
                            </div>
                        </div>
                        <!-- end title -->

                        <div class="col-12">
                            <% for (let song of prevSeasonTopSongs) { %>
                                <div id="season-album">
                                    <div>
                                        <div class="album__cover">
                                            <iframe width="560" height="315" src="https://www.youtube.com/embed/<%= song.youtubeSongId %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                                        </div>
                                        <div class="album__title" id="song-season-details">
                                            <div>
                                                <h3><a href="/release/<%= song.songTitle %>/<%= song.artistName %>"><%= song.songTitle %></a></h3>
                                                <span><a href="/artist/<%= song.artistName %>"><%= song.artistName %></a></span>
                                            </div>
                                            <div>
                                                <span class="single-item__time top-votes">Votes: <%= song.votes %></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>
                        </div>
					</div>
				</div>
			</section>

			<section class="row row--grid">
				<!-- title -->
				<div class="col-12">
					<div class="main__title season-title">
						<h2>New Season Contestants (Dec 1st - Dec 31st)</h2>

						<a href="/artists" class="main__link">See all <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M17.92,11.62a1,1,0,0,0-.21-.33l-5-5a1,1,0,0,0-1.42,1.42L14.59,11H7a1,1,0,0,0,0,2h7.59l-3.3,3.29a1,1,0,0,0,0,1.42,1,1,0,0,0,1.42,0l5-5a1,1,0,0,0,.21-.33A1,1,0,0,0,17.92,11.62Z"/></svg></a>
					</div>
				</div>
				<!-- end title -->
				<div class="col-12">
					<div class="main__carousel-wrap">
						<div class="main__carousel main__carousel--artists owl-carousel" id="artists">
							<% for (let artist of artists) { %>
								<% if (artist.userType === 'Contestant') { %>
									<a href="/artist/<%= artist.name %>" class="artist" id="find-artist">
										<div class="artist__cover">
											<img src="<%= artist.userProfileImage %>" alt="">
										</div>
										<h3 class="artist__title"><%= artist.name %></h3>
									</a>
								<% } %>
							<% } %>
						</div>
					</div>
				</div>
			</section>
		</div>
	</main>
	<!-- end main content -->
<%- include('../includes/end.ejs', {ads: ads}) %>
