<%- include('../includes/head.ejs') %>
<%- include('../includes/navigation.ejs') %>
	<!-- main content -->
	<main class="main">
		<section class="row row--grid">
			<div class="col-12 col-md-6 col-xl-11 season-container">
				<div class="row row--grid">
					<!-- title -->
					<div class="col-12">
						<div class="main__title season-title">
							<h2><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"><path d="M21.65,2.24a1,1,0,0,0-.8-.23l-13,2A1,1,0,0,0,7,5V15.35A3.45,3.45,0,0,0,5.5,15,3.5,3.5,0,1,0,9,18.5V10.86L20,9.17v4.18A3.45,3.45,0,0,0,18.5,13,3.5,3.5,0,1,0,22,16.5V3A1,1,0,0,0,21.65,2.24ZM5.5,20A1.5,1.5,0,1,1,7,18.5,1.5,1.5,0,0,1,5.5,20Zm13-2A1.5,1.5,0,1,1,20,16.5,1.5,1.5,0,0,1,18.5,18ZM20,7.14,9,8.83v-3L20,4.17Z"/></svg><a>Top 6 Singles of Season <%= seasonNumber %></a></h2>
						</div>
					</div>
					<!-- end title -->

					<div class="col-12">
						<% for (let song of topFiveSongs) { %>
							<div id="season-albums">
								<div>
									<div class="album__cover">
										<iframe width="560" height="315" src="https://www.youtube.com/embed/<%= song.youtubeSongId %>" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen loading="lazy"></iframe>
									</div>
									<div class="album__title" id="song-season-details">
										<div>
											<h3><a href="/release/<%= song.songTitle %>/<%= song.artistName %>"><%= song.songTitle %></a></h3>
											<span><a href="/artist/<%= song.artistName %>"><%= song.artistName %></a></span>
										</div>
										<div>
											<span class="single-item__time" id="top-votes">Votes: <%= song.votes %></span>
										</div>
									</div>
								</div>
							</div>
						<% } %>
					</div>
				</div>
			</div>
		</section>
	</main>
	<!-- end main content -->
<%- include('../includes/end.ejs', {ads: ads}) %>